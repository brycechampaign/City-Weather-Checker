{
  "name": "city-weather-checker",
  "version": "1.0.0",
  "description": "A single-page web application using React JS that allows users to look up current weather information for cities around the world and add cities to a “favorites” list.",
  "main": "index.js",
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/src/tests/setupTests.js"
    ],
    "verbose": true
  },
  "scripts": {
    "test": "jest",
    "build-dev": "webpack -d --mode development --watch",
    "build": "webpack -p --mode production",
    "start": "node src/server/index.js",
    "start-dev": "nodemon src/server/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brycechampaign/City-Weather-Checker.git"
  },
  "keywords": [
    "React",
    "Weather"
  ],
  "author": "Bryce Champaign",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/brycechampaign/City-Weather-Checker/issues"
  },
  "homepage": "https://github.com/brycechampaign/City-Weather-Checker#readme",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^4.3.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "jest": "^26.4.2",
    "style-loader": "^1.2.1",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-regular-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "axios": "^0.20.0",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.19.0",
    "core-js": "^3.6.5",
    "express": "^4.17.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "regenerator-runtime": "^0.13.7"
  }
}
